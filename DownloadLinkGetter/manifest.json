{
  // 必须的字段
  "name": "DownloadLinkGetter",
  "version": "1.0.0",
  "manifest_version": 2,
  // 建议提供的字段
  "description": "Download helper",
 // "default_locale": "en",
  // 多选一，或者都不提供c
  "permissions": [ "contextMenus","tabs", "\u003Call_urls\u003E"],//, "downloads", "webRequest","webRequestBlocking", "tabs", "\u003Call_urls\u003E" ],
  "icons": {
    "16":"img/favicon.ico",
    "32":"img/favicon.ico"
  },
  "web_accessible_resources": ["js/inject.js", "js/jquery-3.2.1.js"],
  "background": {
    "scripts": ["js/menu.js", "js/jquery-3.2.1.js"]
  },
  "offline_enabled": true
}